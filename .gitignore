# Sistem Ujian dan Pekerjaan Rumah (UJAN)
# Git ignore file

# Configuration files with sensitive data
config/database.php
config/ai_config.php
*.env
.env.local

# Uploads and user content
assets/uploads/*
!assets/uploads/.gitkeep
assets/uploads/soal/*
!assets/uploads/soal/.gitkeep
assets/uploads/pr/*
!assets/uploads/pr/.gitkeep
assets/uploads/profile/*
!assets/uploads/profile/.gitkeep

# Backups
backups/*
!backups/.gitkeep
*.sql
*.zip
*.tar.gz

# Logs
*.log
logs/*
!logs/.gitkeep

# Cache
cache/*
!cache/.gitkeep
tmp/*
!tmp/.gitkeep

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Composer
vendor/
composer.lock

# Node modules (if using npm)
node_modules/
package-lock.json

# System files
.htaccess.local
.htpasswd

# Session files
sessions/*
!sessions/.gitkeep

# Temporary files
*.tmp
*.temp

# Test and helper files (development only)
test.php
test_*.php
*_test.php
setup_git*.php
push_to_github.php
git_setup.ps1
setup_git.ps1
push_to_github.ps1
migrate_*.php
cleanup_*.php
cleanup_*.ps1
*.ps1

# Documentation (keep only README.md)
*.md
!README.md

# Migration files (keep database.sql, ignore migration scripts)
migration_*.sql
migrate_*.php

# API operations (development)
api/github_operations.php

